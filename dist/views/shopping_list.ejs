<div class="confirm-checkout-container">
    <div class="confirm-checkout-content">
        <div class="confirm-checkout-header">
            <i class="fa fa-check-circle-o"></i>
            <p>Confirm</p>
        </div>
        <div class="confirm-checkout-title">
            <p>Are you sure you want to checkout?</p>
        </div>
        <div class="confirm-checkout-body">
            <button class="cancel-checkout-btn" onclick="closeConfirmCheckout()">Cancel</button>
            <button class="confirm-checkout-btn" onclick="checkout()">Checkout</button>
            <!-- loading icon -->
            <i class="fa fa-spinner fa-spin fa-1x fa-fw" style="display:none"></i>
        </div>
    </div>
</div>

<div class="checkout-feedback-msg-container">
    <div class="checkout-feedback-msg">
        <i class="fa fa-check-circle-o"></i>
        <i class="fa fa-exclamation-triangle" style="display:none"></i>
        <p>Checkout Successful</p>
    </div>
</div>

<div id="shop-list-container">
    <div class="shop-list-wrapper">
        <div class="cart-error-msg">
            <p>No items in cart</p>
        </div>
        <div class="shop-list-content">
            <div class="shop-list-header">
                <!-- <a class="cross-shop-list" onclick="closeList()">&times;</a> -->
                <div class="shop-list-title">
                    <a href="javascript:void(0)" class="shop-logo-wrapper" onclick="closeShopList()">&times;</a>
                    <h3>Your Items</h3>
                </div>
                <div class="shop-item-header">
                    <p>Product</p>
                    <p>Quantity</p>
                    <p>Price</p>
                    <p>SubTotal</p>
                </div>
            </div>
            
        </div> 
        <div class="checkout">
            <button class="checkout-btn" onclick="confirmPurchase()">Proceed to Checkout</button>
            <p>$<span>0</span></p>
        </div>
    </div>
</div>