            <div class="category-container">
                <div class="category-content">
                    <h2 class = "featured-products"> Best Sellers </h2>
                    <div class="row">
                        <% for (let i = 0; i < 4; i ++) { %>
                            <div class="product-id data-product-id" style="display:none"><%= data.bestSellers[i].id %></div>
                            <div class="col-4">
                                <div class="col-4-header">
                                    <div class="col-4-header-heart">
                                        <i class="fa fa-heart-o"></i>
                                        <i class="fa fa-heart"></i>
                                    </div>
                                </div>
                                <div class="product-item-container">
                                    <img src="<%= data.bestSellers[i].imagesource %>" onclick="previewBtnClicked('<%= data.newProducts[i].id %>')" class="featured-overlay-image data-item-image data-preview-btn" alt="adidas_shoes">
                                    <div class="product-content">
                                        <% for (let j = 0; j < data.bestSellers[i].rating; j++) { %>
                                            <i class="fa fa-star"></i>
                                        <% } %>

                                        <% if (data.bestSellers[i].rating !== parseInt(data.bestSellers[i].rating)) { %>
                                            <i class="fa fa-star-half-o"></i>
                                        <% } %>

                                        <% for (let j = 0; j < (5 - data.bestSellers[i].rating); j++) { %>
                                            <i class="fa fa-star-o"></i>
                                        <% } %>

                                        <div class="product-item-details">
                                            <div class="left-hand">
                                                <div class="product-name data-item-name"> <%= data.bestSellers[i].name %> </div>
                                                <div class="product-price data-item-price">$<%= data.bestSellers[i].price %> </div>
                                            </div>
                                            <div class="featured-overlay-cart-container">
                                                <i class="fa fa-shopping-cart featured-overlay-cart" onclick="pressShoppingCart('<%= data.bestSellers[i].id %>', '<%= data.bestSellers[i].name %>', '<%= data.bestSellers[i].price %>', '<%= data.bestSellers[i].imagesource %>')"></i>
                                            </div>
                                            <div class="featured-quantity">
                                                <i class="fa fa-minus-square featured-minus-square"></i>
                                                <div class="featured-product-counter">0</div>
                                                <i class="fa fa-plus-square featured-plus-square"></i>
                                            </div>
                                        </div>
                                        <button class="preview-item-btn" onclick="previewBtnClicked('<%= data.newProducts[i].id %>')">Preview Item</button>
                                    </div>
                                </div>
                            </div>

                        <% } %>

                    </div>
                </div>
            </div>

